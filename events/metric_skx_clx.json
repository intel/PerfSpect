		},	
		{	
			"name"       : "metric_TMAM_....ICache_Misses(%)",
			"expression" : "100 * ([ICACHE_16B.IFDATA_STALL] + 2 * [ICACHE_16B.c1_e1_IFDATA_STALL]) /  [cpu-cycles]"
