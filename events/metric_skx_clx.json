		},
		{
			"name"	     : "metric_TMAM_......MEM_Bandwidth(%)",
			"expression" : "100 * min([OFFCORE_REQUESTS_OUTSTANDING.L3_MISS_DEMAND_DATA_RD_GE_6] , [cpu-cycles]) / [cpu-cycles]"
