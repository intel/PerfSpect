		},
		{	
			"name"       : "metric_TMAM_Info_CoreIPC",
			"expression" : "[instructions] / ([CPU_CLK_UNHALTED.THREAD_ANY] / [const_thread_count])"
		},
		{	
			"name"       : "metric_TMAM_Frontend_Bound(%)",
